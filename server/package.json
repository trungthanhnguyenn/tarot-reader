{
  "name": "tarot-reader-server",
  "version": "1.0.0",
  "description": "Backend server for Tarot Reader application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts}\" --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "clean": "rimraf dist"
  },
  "keywords": [
    "tarot",
    "express",
    "typescript",
    "openai",
    "sqlite"
  ],
  "author": "Tarot Reader Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "crypto": "^1.0.1",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
